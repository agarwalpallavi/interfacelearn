<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0" xmlns="http://java.sun.com/xml/ns/javaee">

	<!-- Servlets -->
	<servlet>
		<servlet-name>greetServlet</servlet-name>
		<servlet-class>com.interfacelearn.server.GreetingServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>greetServlet</servlet-name>
		<url-pattern>/greet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>selectionServlet</servlet-name>
		<servlet-class>com.interfacelearn.server.SelectionImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>selectionServlet</servlet-name>
		<url-pattern>/selection</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>startTestServlet</servlet-name>
		<servlet-class>com.interfacelearn.server.StartTest</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>startTestServlet</servlet-name>
		<url-pattern>/starttest</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>startTestServlet2</servlet-name>
		<servlet-class>com.interfacelearn.server.StartTest2</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>startTestServlet2</servlet-name>
		<url-pattern>/starttest2</url-pattern>
	</servlet-mapping>
	
	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>Com_interfacelearn.html</welcome-file>
	</welcome-file-list>

</web-app>
